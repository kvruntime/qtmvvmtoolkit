{
  "name": "qtmvvmtoolkit",
  "dockerComposeFile": [
    "./docker-compose.yml"
  ],
  "workspaceFolder": "/workspaces/qtmvvmtoolkit",
  "service": "qtmvvmtoolkit",
  "features": {
    "ghcr.io/nils-geistmann/devcontainers-features/zsh:0": {
      "setLocale": true,
      "theme": "agnoster",
      "plugins": "git docker",
      "desiredLocale": "en_US.UTF-8 UTF-8"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  },
  "postCreateCommand": "pip install -r requirements.txt"
}